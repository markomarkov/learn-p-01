{
  "name": "game-state-service",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/game-state-service/src",
  "tags": [],
  "// targets": "to see all targets run: nx show project game-state-service --web",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/game-state-service",
        "main": "apps/game-state-service/src/main.ts",
        "tsConfig": "apps/game-state-service/tsconfig.app.json",
        "webpackConfig": "apps/game-state-service/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    }
  }
}
